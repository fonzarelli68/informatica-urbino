	<!-- carousel1 script & params -->
	<script type="text/JavaScript">
	$(document).ready(function(){
	$("#carousel1").CloudCarousel({
		xRadius: 570,
		yRadius: 160,
		xPos: 640,
		yPos: 160,
		titleBox: $('#alberi-title'),
		altBox: $('#alberi-alt'),
		reflHeight: 110,
		reflGap: 3,
		reflOpacity: 0.3,
		speed: 0.25,
		mouseWheel: true,
		buttonLeft: $("#left-but"),
		buttonRight: $("#right-but")
	});
});
	</script>
	<style>
	body {
		background-color: #000000;
	}
	
	#carousel1 {
		margin: auto;
	}
	
	#carousel1 img {
		border: none;
		max-height: 380px;
	}
	
	#ctitle {
		color: white;
		text-align: center;
		font-family: Arial, sans-serif;
	}
	
	#ctitle a {
		color: yellow;
		border: none;
		text-decoration: none;
	}
	
	#ctitle a:hover {
		color: red;
		border: none;
		text-decoration: none;
	}
	
	.dir {
		width:30px;
		height:30px;
		background:url(https://informatica-urbino.googlecode.com/svn/trunk/fanfulla/carousel/nivo-arrows.png) no-repeat;
		text-indent:-9999px;
		border:0;
	}

	#right-but {
		background-position: -30px 0;
		position: absolute;
		top: 10px;
		right: 15px;
		display: inline;
	}

	#left-but {
		position: absolute;
		top: 10px;
		left: 15px;
		display: inline;
	}

	#alberi-title {
